[campaigndb]
	[campaigndb.storage]
		[campaigndb.storage.compression]
			enabled = true
[contentindex]
	[contentindex.adverts]
		enabled = true
	[contentindex.curated]
		enabled = true
	[contentindex.news]
		hide-read = false
[instrumentation]
	[instrumentation.tracy]
		enabled = false
		port = 8086
[masterserver]
	[masterserver.key-authentication]
		enforce-on-private-networks = false
		mode = "always"
[net]
	[net.http]
		max-concurrent-connections = 20
		[net.http.disk-cache]
			enabled = true
			size = 100
			[net.http.disk-cache.compression]
				enabled = true
		[net.http.ssl]
			cabundle = ""
			capath = ""
			require = false
	[net.udp]
		[net.udp.window]
			timeout = 60000
[nwscript]
	[nwscript.debugger]
		address = "127.0.0.1"
		enabled = false
		port = 5122
	[nwscript.nwnx]
		[nwscript.nwnx.handler]
			abort-script-without-nwnx = true
[ruleset]
	always-roll-max-hitpoints-on-levelup = true
	difficulty = 3
	enforce-legal-characters = false
	hide-hitpoints-gained-on-levelup = false
	item-level-restrictions = false
	max-character-level = 20
	min-character-level = 1
	natural-ones-fail-saving-throws = true
	show-active-effects = false
	show-challenge-ratings = false
	validate-spells = true
	[ruleset.combat]
		max-ability-bonus = 12
		max-ability-penalty = 12
		max-attack-bonus = 20
		max-damage-bonus = 100
		max-saving-throw-bonus = 20
		max-skill-bonus = 50
		sticky-modes = false
	[ruleset.encounters]
		always-reset = false
[server]
	allow-player-pausing = false
	disallow-shouting = false
	one-party-only = false
	player-party-control = false
	pvp-mode = 2
	reload-when-empty = false
	restore-spell-uses-on-login = false
	savegames-store-characters = true
	savegames-vault-characters-only = false
	show-dm-join-messages = true
	show-player-join-messages = true
	[server.autosave]
		enabled = false
		interval = 5
	[server.identity]
		game-region = 0
		game-type = 0
		name = "Server"
		public = true
	[server.log]
		[server.log.creature-deaths]
			enabled = false
		[server.log.debug-mode-sends-to-players]
			enabled = true
		[server.log.rotation]
			enabled = true
	[server.login]
		admin-password = ""
		dm-password = ""
		max-players = 6
		player-password = ""
		[server.login.player-dm]
			enabled = true
	[server.net]
		accept-relayed-connections = true
		port = 5121
	[server.tweaks]
		2da-engine-cache-size = 11
		2da-user-cache-size = 10
		game-obj-update-interval = 200000
		game-obj-update-interval-loading = 20000
		game-object-cache-size = 16
		message-limit = 400
		message-limit-loading = 2000
		threaded-timers-resolution = 100
		use-threaded-timers = true
		[server.tweaks.power-saving]
			clients-required-to-disable = 1
		[server.tweaks.resman]
			max-memory-usage = 0.12500000000000000
	[server.vault]
		backup-server-characters = false
		by-player-name = false
		mode = 1
		sticky-player-names = false
		suppress-base = false
[sqlite3]
	[sqlite3.busy-timeout]
		milliseconds = 3000
	[sqlite3.trace]
		[sqlite3.trace.profile]
			enabled = false
			threshold-msec = 800
		[sqlite3.trace.stmt]
			enabled = false
[taskmanager]
	[taskmanager.threadpool]
		max-mutex-wait-time = 5000
		max-size = 0
